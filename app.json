{
  "expo": {
    "name": "LockDigit",
    "slug": "lockdigit",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "lockdigit",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": false,
    "splash": {
      "image": "./assets/images/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#0A0E1A"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.lockdigit.app",
      "googleServicesFile": "./GoogleService-Info.plist"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#0A0E1A"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false,
      "package": "com.lockdigit.app",
      "googleServicesFile": "./google-services.json",
      "permissions": [
        "android.permission.CAMERA",
        "android.permission.RECORD_AUDIO"
      ]
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "@react-native-firebase/app",
      "@react-native-firebase/auth",
      [
        "@stripe/stripe-react-native",
        {
          "merchantIdentifier": "merchant.com.lockdigit.app",
          "enableGooglePay": true
        }
      ],
      "expo-router",
      "expo-secure-store",
      [
        "expo-camera",
        {
          "cameraPermission": "Allow LockDigit to access your camera for scanning documents and QR codes."
        }
      ],
      [
        "expo-image-picker",
        {
          "photosPermission": "Allow LockDigit to access your photos to upload identity documents."
        }
      ],
      [
        "expo-notifications",
        {
          "color": "#6C63FF"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "af9555f6-1864-42b9-a0b1-9f2094ffc546"
      }
    },
    "owner": "lochanreddy"
  }
}
